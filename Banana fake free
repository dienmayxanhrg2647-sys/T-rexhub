-- [[ WAKE HUB - BANANA LOADER + SUPER ANTI-LAG ]] --

repeat task.wait() until game:IsLoaded() and game.Players.LocalPlayer

-- 1. H·ªÜ TH·ªêNG SI√äU C·∫§P ANTI-LAG (H·ªßy di·ªát lag)
local function ActivateAntiLag()
    local g = game
    local w = g.Workspace
    local l = g.Lighting
    
    -- T·ªëi ∆∞u Lighting
    l.GlobalShadows = false
    l.FogEnd = 9e9
    l.Brightness = 0
    settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
    
    -- X·ª≠ l√Ω Object ƒë·ªÉ tƒÉng FPS
    for _, v in pairs(g:GetDescendants()) do
        if v:IsA("Part") or v:IsA("UnionOperation") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") then
            v.Material = Enum.Material.Plastic
            v.Reflectance = 0
        elseif v:IsA("Decal") or v:IsA("Texture") then
            v.Transparency = 1
        elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
            v.Enabled = false
        elseif v:IsA("Explosion") then
            v.Visible = false
        end
    end
    
    -- Gi·∫£m t·∫£i x·ª≠ l√Ω ƒë·ªãa h√¨nh
    local terrain = w:FindFirstChildOfClass("Terrain")
    if terrain then
        terrain.WaterWaveSize = 0
        terrain.WaterWaveSpeed = 0
        terrain.WaterReflectance = 0
        terrain.WaterTransparency = 0
    end
    
    print("WAKE HUB: Anti-Lag Activated! FPS Boosted.")
end

-- Ch·∫°y Anti-Lag tr∆∞·ªõc
task.spawn(ActivateAntiLag)

-- 2. TH√îNG B√ÅO GIAO DI·ªÜN
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "ü¶ñ T-REX X WAKE",
    Text = "Anti-Lag On! Loading Banana G·ªëc...",
    Duration = 5
})

-- 3. K√çCH HO·∫†T BANANA G·ªêC (Link ch√≠nh ch·ªß)
local function LoadBanana()
    local success, err = pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/kimprobloxdz/Banana-Free/refs/heads/main/Protected_5609200582002947.lua.txt"))()
    end)
    
    if not success then
        warn("L·ªói t·∫£i Banana: " .. tostring(err))
    end
end

-- ƒê·ª£i 1 ch√∫t cho Anti-Lag ·ªïn ƒë·ªãnh r·ªìi load script
task.delay(1, LoadBanana)
