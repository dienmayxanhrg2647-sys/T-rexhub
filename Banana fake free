-- [[ 1. L√ïI B·∫∫ KH√ìA (CH·∫∂N ƒê√íI KEY) ]] --
getgenv().Key = "TREX_X_FREE"
getgenv().BananaHubKey = "TREX_X_FREE"

-- Hook h·ªá th·ªëng: √âp script Banana lu√¥n nh·∫≠n k·∫øt qu·∫£ l√† "ƒê√£ c√≥ Key"
local oldHttpGet;
oldHttpGet = hookfunction(game.HttpGet, function(self, url, ...)
    if url:find("key") or url:find("verify") or url:find("check") then
        return '{"status":"success","valid":true,"key":"TREX_X_BYPASS"}'
    end
    return oldHttpGet(self, url, ...)
end)

-- [[ 2. LOADER T·ª∞ ƒê·ªòNG V√ÄO TH·∫≤NG MENU ]] --
task.spawn(function()
    local HttpService = game:GetService("HttpService")
    local PlaceId = game.PlaceId
    
    -- L·∫•y UniverseID ƒë·ªÉ nh·∫≠n di·ªán game ch√≠nh x√°c
    local UniverseID = HttpService:JSONDecode(game:HttpGet("https://apis.roblox.com/universes/v1/places/"..PlaceId.."/universe")).universeId

    -- T·ª± ƒë·ªông n·∫°p script t∆∞∆°ng ·ª©ng (ƒê√£ b·∫ª kh√≥a)
    if PlaceId == 7449423635 or PlaceId == 2753915549 or PlaceId == 4442272183 or UniverseID == 994732206 then
        -- Blox Fruit
        loadstring(game:HttpGet("https://raw.githubusercontent.com/obiiyeuem/vthangsitink/main/BF-BananaCat.lua"))()
    elseif PlaceId == 18901165922 or PlaceId == 19006211286 then 
        -- Pets Go
        loadstring(game:HttpGet("https://raw.githubusercontent.com/obiiyeuem/vthangsitink/refs/heads/main/PetsGo.lua"))()
    elseif PlaceId == 16732694052 then 
        -- Fisch
        loadstring(game:HttpGet("https://raw.githubusercontent.com/AhmadV99/Banana-Cat-Hub/main/Fisch.lua"))()
    elseif UniverseID == 5844593548 then 
        -- Anime Reborn
        loadstring(game:HttpGet("https://raw.githubusercontent.com/obiiyeuem/vthangsitink/main/AnimeReborn.lua"))()
    else
        -- C√°c game kh√°c (King Legacy, Bee Swarm, v.v.)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/obiiyeuem/vthangsitink/main/AV-BananaCat.lua"))()
    end
end)
